{
  "name": "cyber-escape-room",
  "version": "2.0.77",
  "description": "Cyberpunk-themed Offensive Security Escape Room",
  "private": true,
  "scripts": {
    "main": "node main.js",
    "start": "node main.js",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ..",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "verify": "node backend/verify-startup.js",
    "check": "node backend/check-config.js",
    "clean": "rm -rf backend/node_modules frontend/node_modules node_modules",
    "setup": "npm run install:all && chmod +x launch.sh && chmod +x main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/offensive-sec-quiz-2.git"
  },
  "keywords": [
    "cybersecurity",
    "offensive-security",
    "escape-room",
    "cyberpunk",
